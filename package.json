{
  "name": "react-dom-outside",
  "version": "0.1.0",
  "description": "React components for react-dom and react-native-web click outside",
  "keywords": [
    "react",
    "web",
    "react-dom",
    "dom",
    "contains",
    "outside",
    "universal",
    "typescript"
  ],
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "files": [
    "dist"
  ],
  "tsds": {
    "source": "src/index.ts",
    "globals": {
      "react": "React",
      "react-dom-event": "reactDomEvent",
      "react-dom-contains": "reactDomContains",
      "react-ref-boundary": "ReactRefBoundary"
    }
  },
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "types": "dist/types/index.d.ts",
  "repository": "kmalakoff/react-dom-outside",
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "format": "tsds format",
    "lint": "tsds lint",
    "test": "tsds test:node && tsds test:browser",
    "version": "tsds version"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "@types/react": "18.0.15",
    "@types/react-dom": "^18.0.6",
    "jsdom": "^15.2.1",
    "react": "18.2.x",
    "react-dom": "18.2.x",
    "react-dom-event": "^0.1.2",
    "react-native-web": "0.18.4",
    "react-ref-boundary": "^0.1.2",
    "ts-dev-stack": "^0.6.6"
  },
  "peerDependencies": {
    "react": "*"
  },
  "optionalDependencies": {
    "react-dom-event": "*",
    "react-ref-boundary": "*"
  },
  "engines": {
    "node": ">=4"
  },
  "unpkg": "dist/umd/react-dom-outside.js"
}
