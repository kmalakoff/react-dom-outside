{
  "name": "react-dom-outside",
  "version": "0.1.2",
  "description": "React components for react-dom and react-native-web click outside",
  "keywords": [
    "react",
    "web",
    "react-dom",
    "dom",
    "contains",
    "outside",
    "universal",
    "typescript"
  ],
  "repository": "kmalakoff/react-dom-outside",
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.js",
  "unpkg": "dist/umd/react-dom-outside.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "format": "biome check --apply src/ test/",
    "test:node": "tsds link && tsx node_modules/.bin/mocha test/**/*.tsx",
    "test": "npm run test:node && tsds test:browser",
    "test:engines2": "nvu engines tsds test:node && tsds test:browser",
    "version": "tsds version"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.17",
    "@types/react": "18.2.55",
    "@types/react-dom": "^18.2.19",
    "jsdom": "^9.12.0",
    "react": "18.2.x",
    "react-dom": "18.2.x",
    "react-dom-event": "^0.1.2",
    "react-native-web": "0.18.4",
    "react-ref-boundary": "^0.1.2",
    "ts-dev-stack": "^0.8.12",
    "tsx": "^4.7.1"
  },
  "peerDependencies": {
    "react": "*"
  },
  "optionalDependencies": {
    "react-dom-event": "*",
    "react-ref-boundary": "*"
  },
  "engines": {
    "node": ">=4"
  },
  "tsds": {
    "source": "src/index.ts",
    "globals": {
      "react": "React",
      "react-dom-event": "reactDomEvent",
      "react-dom-contains": "reactDomContains",
      "react-ref-boundary": "ReactRefBoundary"
    }
  }
}
